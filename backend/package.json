{
  "name": "repoai-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev:server": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "dev": "concurrently -n \"API,WORKER\" -c \"blue,magenta\" \"npm run dev:server\" \"npm run dev:worker\""
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.66.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.x",
    "dotenv": "^16.6.1",
    "express": "^4.x",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "openai": "^6.10.0",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.x",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.x",
    "@types/unzipper": "^0.10.11",
    "concurrently": "^8.2.2",
    "ts-node-dev": "^2.x",
    "typescript": "^5.x"
  }
}
